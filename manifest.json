{
    "name": "Vipin's Extension",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts" : [
        {
            "matches" : ["https://www.codechef.com/*/submit/*"],
            "run_at": "document_idle",
            "all_frames": true,
            "js" : ["contentScript.js"]
        }
    ],
    "background": {
        "scripts": ["background.js", "popup.js", "jQuery-3.6.0.js"],
        "persistent": true
    },
   "permissions": ["storage",
        "webRequest",
        "tabs",
        "activeTab",
        "https://www.codechef.com/*"
        
    ],
    "browser_action": {
        "default_icon": "Vipin.jpg",
        "default_popup" : "popup.html"
        
    }
  }